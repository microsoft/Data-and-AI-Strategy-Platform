{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
      "storageAccountType": {
        "value": "Standard_LRS"
      },
      "accessTier": {
        "value": "Hot"
      },
      "isHnsEnabled": {
        "value": false
      },
      "requireInfrastructureEncryption": {
        "value": true
      },
      "softDeleteEnabled": {
        "value": false
      },
      "autoDeletePolicy": {
        "value": false
      },
      "allowBlobPublicAccess": {
        "value": true
      },
      "DataLakeContainerNames": {
        "value": [
          "config",
          "images"
        ]
      },
      "deployblobPE": {
        "value": true
      },
      "deploydfsPE": {
        "value": false
      },
      "deployfilePE": {
        "value": true
      },
      "deployqueuePE": {
        "value": true
      },
      "deploytablePE": {
        "value": true
      },
      "enableDiagnostics": {
        "value": false
      }
    }
  }